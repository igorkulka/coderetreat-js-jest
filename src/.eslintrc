{
  "rules": {
    "no-console": false,
    "array-type": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "quotemark": false,
    "arrow-parens": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "//"
      }
    ],
    "ordered-imports": false
  }
}
